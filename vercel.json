{
  "version": 2,
  "builds": [
    {
      "src": "api/ask.py",
      "use": "@vercel/python"
    },
    {
      "src": "public/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/ask",
      "dest": "api/ask.py",
      "methods": ["POST"]
    },
    {
      "src": "/(.*)",
      "dest": "public/index.html"
    }
  ]
}