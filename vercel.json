{
  "version": 2,
  "builds": [
    {
      "src": "api/aiprompt.py",
      "use": "@vercel/python"
    },
    {
      "src": "static/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "static/$1"
    },
    {
      "src": "/ask",
      "dest": "api/aiprompt.py",
      "methods": ["POST"]
    },
    {
      "src": "/(.*)",
      "dest": "static/index.html"
    }
  ]
}